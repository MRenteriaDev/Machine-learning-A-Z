#See the SVR Model
library(ggplot2)
x_grid = seq(min(dataset$Level), max(dataset$Level), 0.1)
ggplot() +
geom_point(aes(x= dataset$Level, y = dataset$Salary),
color = "red") +
geom_line(aes(x = x_grid, y = predict(regression, newdata = data.frame(Level = x_grid))),
color = "blue") +
gtitle("Prediction (SVR)") +
